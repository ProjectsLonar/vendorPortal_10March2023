listActiveLikeNameByCategory = select p.PRODUCT_ID,p.PRODUCT_CODE,p.PRODUCT_NAME,p.UOM,p.MIN_ORDER_QTY,p.MAX_ORDER_QTY,p.CATEGORY_ID,  \
p.PRODUCT_TYPE,p.GL_ACCOUNT_ID,p.LINE_TYPE,p.PRODUCT_IMAGE, p.START_DATE,p.END_DATE,p.HSN_SAC_CODE,p.PRODUCT_DESC, p.SUB_CATEGORY_ID \
FROM LT_MAST_PRODUCTS p WHERE (upper(p.PRODUCT_NAME) like ? OR upper(p.PRODUCT_CODE) like ? )  \
AND p.CATEGORY_ID = ? AND nvl(p.SUB_CATEGORY_ID,-99) like nvl(?, nvl(p.SUB_CATEGORY_ID,-99))



getProductDatatableRecords = select  *  from  \
(SELECT a.*,ROWNUM RNUM from ( select ga.PRODUCT_ID,ga.PRODUCT_CODE,ga.PRODUCT_NAME,cmv.UNIT_NAME as UOM_VALUE,ga.PRODUCT_TYPE, \
ga.START_DATE,ga.END_DATE  \
FROM  LT_MAST_PRODUCTS ga  ,LT_MAST_UNIT cmv  \
WHERE  ga.UOM = cmv.UNIT_ID(+)  \
AND ga.COMPANY_ID = ? \
AND upper( ga.PRODUCT_CODE) LIKE  nvl(?,upper( ga.PRODUCT_CODE))   \
AND  upper( ga.PRODUCT_NAME) LIKE  nvl(?,upper( ga.PRODUCT_NAME))  \
AND  upper( cmv.UNIT_NAME) LIKE  nvl(?,upper( cmv.UNIT_NAME)) \
AND  upper( ga.PRODUCT_TYPE) LIKE  nvl(?,upper( ga.PRODUCT_TYPE)) \
AND  TO_CHAR(ga.START_DATE,'DD-MM-YYYY') LIKE nvl(?, TO_CHAR(ga.START_DATE,'DD-MM-YYYY'))  \
AND  TO_CHAR(nvl (ga.END_DATE, SYSDATE ),'DD-MM-YYYY') LIKE nvl(?, TO_CHAR(nvl (ga.END_DATE, SYSDATE ),'DD-MM-YYYY'))  \
order by \
CASE WHEN 2 = ? then upper(ga.PRODUCT_CODE) END asc, \
CASE WHEN 12 = ? then upper(ga.PRODUCT_CODE) END desc, \
CASE WHEN 3 = ? then upper( ga.PRODUCT_NAME) END asc, \
CASE WHEN 13 = ? then upper(ga.PRODUCT_NAME) END desc, \
CASE WHEN 4 = ? THEN upper(cmv.UNIT_NAME) END asc, \
CASE WHEN 14 = ? then upper(cmv.UNIT_NAME) END desc, \
CASE WHEN 5 = ? THEN upper(ga.PRODUCT_TYPE) END asc, \
CASE WHEN 15 = ? then upper(ga.PRODUCT_TYPE) END desc,\
CASE WHEN 6 = ? then ga.START_DATE END asc,  \
CASE WHEN 16 = ? then ga.START_DATE END desc, \
CASE WHEN 7 = ? then ga.END_DATE END asc, \
CASE WHEN 17 = ? then ga.END_DATE END desc,\
CASE WHEN 8 = ? then ga.last_update_date END desc,  \
CASE WHEN 18 = ? then ga.last_update_date END asc) a  where ROWNUM <=?)  WHERE RNUM >=?


