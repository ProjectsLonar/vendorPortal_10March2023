findActiveProductsLikeName = select p.PRODUCT_ID,p.PRODUCT_CODE,p.PRODUCT_NAME,p.PRODUCT_DESC,p.UOM,p.MIN_ORDER_QTY,p.MAX_ORDER_QTY, \
