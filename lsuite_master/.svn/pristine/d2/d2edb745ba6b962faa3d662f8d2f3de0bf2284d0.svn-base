WHERE  ga.UOM = cmv.UNIT_ID(+)  \
AND ga.COMPANY_ID = ? \
AND upper( ga.PRODUCT_CODE) LIKE  nvl(?,upper( ga.PRODUCT_CODE))   \
AND  upper( ga.PRODUCT_NAME) LIKE  nvl(?,upper( ga.PRODUCT_NAME))  \
AND  upper( cmv.UNIT_NAME) LIKE  nvl(?,upper( cmv.UNIT_NAME)) \
AND  upper( ga.PRODUCT_TYPE) LIKE  nvl(?,upper( ga.PRODUCT_TYPE)) \
AND  TO_CHAR(ga.START_DATE,'DD-MM-YYYY') LIKE nvl(?, TO_CHAR(ga.START_DATE,'DD-MM-YYYY'))  \
AND  TO_CHAR(nvl (ga.END_DATE, SYSDATE ),'DD-MM-YYYY') LIKE nvl(?, TO_CHAR(nvl (ga.END_DATE, SYSDATE ),'DD-MM-YYYY'))



getLtMastProductsDataForReport = select p.PRODUCT_ID,p.PRODUCT_CODE,p.PRODUCT_NAME,p.PRODUCT_DESC,p.UOM, \
cmv.UNIT_NAME  as  UOM_VALUE, p.MIN_ORDER_QTY, p.MAX_ORDER_QTY,p.CATEGORY_ID,p.SUB_CATEGORY_ID, \
lt_vpal_common_pkg.get_comn_value_name('PRODUCT_TYPE', p.PRODUCT_TYPE)  as PRODUCT_TYPE_VALUE, \
p.PRODUCT_TYPE, p.GL_ACCOUNT_ID, \
lt_vpal_common_pkg.get_comn_value_name('LINE_TYPE', p.LINE_TYPE)  as  LINE_TYPE_VALUE, p.STATUS, \
lt_vpal_common_pkg.get_comn_value_name('STATUS_MASTER', p.STATUS)  as  STATUS_VALUE, \
p.LINE_TYPE,p.PRODUCT_IMAGE,p.HSN_SAC_CODE,p.START_DATE,p.END_DATE, \
p.CREATED_BY,p.CREATION_DATE,p.LAST_UPDATE_LOGIN,p.LAST_UPDATED_BY, p.LAST_UPDATE_DATE, psc.SUB_CATEGORY SUB_CATEGORY_NAME, \
psc.SUB_CATEGORY_CODE, ga.ACCOUNT_CODE GL_ACCOUNT_CODE ,  ga.ACCOUNT_NAME GL_ACCOUNT_NAME ,pc.CATEGORY_NAME,pc.CATEGORY_CODE  \
FROM LT_MAST_PRODUCTS p,LT_MAST_PRODUCT_CATEGORIES pc,LT_MAST_PROD_SUB_CATEGORIES psc,LT_MAST_GL_ACCOUNTS ga ,LT_MAST_UNIT cmv \
WHERE p.CATEGORY_ID = pc.CATEGORY_ID(+) AND  p.SUB_CATEGORY_ID = psc.SUB_CATEGORY_ID(+)  AND p.GL_ACCOUNT_ID = ga.GL_ACCOUNT_ID(+) \
AND  p.status = NVL(?,p.status)  \
AND p.UOM = cmv.UNIT_ID(+) \
AND   p.start_date >= NVL(to_date( ?,'dd-mm-yyyy'),p.start_date)  \
AND   NVL(p.end_date,SYSDATE) <= NVL(to_date( ?,'dd-mm-yyyy'),NVL(p.end_date,SYSDATE))
